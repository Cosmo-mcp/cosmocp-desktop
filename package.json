{
  "name": "CosmoMCP",
  "productName": "CosmoMCP",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "package": "cd src/renderer && npm run build && cd ../.. && set NODE_ENV=production && electron-forge package",
    "make": "cd src/renderer && npm run build && cd ../.. && set NODE_ENV=production && electron-forge make",
    "publish": "cd src/renderer && npm run build && cd ../.. && set NODE_ENV=production && electron-forge publish",
    "lint": "gts lint",
    "fix": "gts fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx src/main/db/migrate.ts",
    "db:studio": "npx drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up"
  },
  "keywords": [],
  "author": {
    "name": "Cosmo-mcp",
    "email": "gaurav@cosmocp.com"
  },
  "license": "AGPL",
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/publisher-github": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "drizzle-kit": "^0.31.5",
    "electron": "^38.2.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "gts": "^6.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@electric-sql/pglite": "^0.3.10",
    "ai": "^5.0.59",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "electron-squirrel-startup": "^1.0.1",
    "zod": "^4.1.11"
  }
}
