{
  "name": "CosmoStudio",
  "productName": "CosmoStudio",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "workspaces": [
    "packages/*",
    "src/renderer"
  ],
  "scripts": {
    "generate-api": "ts-node -r tsconfig-paths/register scripts/generate-api.ts",
    "download-provider-logos": "ts-node scripts/download-provider-logos.ts",
    "next:dev": "cd src/renderer && npm run dev",
    "dev": "concurrently -k \"npm run next:dev\" \"npm run start\"",
    "start": "npm i && cross-env NODE_ENV=development electron-forge start",
    "package": "cd src/renderer && npm run build && cd ../.. && cross-env NODE_ENV=production electron-forge package",
    "make": "cd src/renderer && npm run build && cd ../.. && cross-env NODE_ENV=production electron-forge make",
    "publish": "cd src/renderer && npm run build && cd ../.. && cross-env NODE_ENV=production electron-forge publish",
    "lint": "gts lint",
    "fix": "gts fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": {
    "name": "Cosmo-cp",
    "email": "gaurav@cosmocp.com"
  },
  "license": "AGPL",
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "gts": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.14",
    "@ai-sdk/google": "3.0.9",
    "@ai-sdk/mcp": "1.0.9",
    "@ai-sdk/openai": "3.0.11",
    "@ai-sdk/provider": "^3.0.4",
    "@ai-sdk/xai": "^3.0.57",
    "@electric-sql/pglite": "0.3.15",
    "@stepperize/react": "5.1.9",
    "ai": "6.0.37",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "electron-log": "5.4.3",
    "electron-squirrel-startup": "1.0.1",
    "inversify": "7.11.0",
    "ollama-ai-provider-v2": "3.0.1",
    "reflect-metadata": "0.2.2",
    "update-electron-app": "^3.1.2",
    "zod": "4.3.5"
  }
}
